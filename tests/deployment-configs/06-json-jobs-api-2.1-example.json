{
    "default": {
        "jobs": [
            {
                "name": "dbx_jobs_v21_test",
                "job_clusters": [
                    {
                        "new_cluster": {
                            "spark_version": "9.1.x-cpu-ml-scala2.12",
                            "num_workers": 1,
                            "node_type_id": "{some-node-type-id}"
                        },
                        "job_cluster_key": "basic-cluster"
                    }
                ],
                "tasks": [
                    {
                        "task_key": "first-task",
                        "job_cluster_key": "basic-cluster",
                        "spark_python_task": {
                            "python_file": "file://some/entrypoint.py",
                            "parameters": [
                                "--conf-file",
                                "file:fuse://some/conf/file.yml"
                            ]
                        }
                    },
                    {
                        "task_key": "second-task",
                        "job_cluster_key": "basic-cluster",
                        "spark_python_task": {
                            "python_file": "file://some/entrypoint.py",
                            "parameters": [
                                "--conf-file",
                                "file:fuse://some/conf/file.yml"
                            ]
                        }
                    },
                    {
                        "task_key": "third-task",
                        "job_cluster_key": "basic-cluster",
                        "depends_on": [
                            {
                                "task_key": "first-task"
                            },
                            {
                                "task_key": "second-task"
                            }
                        ],
                        "spark_python_task": {
                            "python_file": "file://some/entrypoint.py",
                            "parameters": [
                                "--conf-file",
                                "file:fuse://some/conf/file.yml"
                            ]
                        }
                    }
                ]
            }
        ]
    }
}
